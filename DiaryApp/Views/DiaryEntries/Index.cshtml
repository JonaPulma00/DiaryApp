@model List<DiaryEntry>

<div>
	<table>
		<thead>
			<tr>
				<th>Title</th>
				<th>Content</th>
				<th>Date</th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			@foreach(var entry in Model)
			{
				<tr>
					<td>@entry.Title</td>
					<td>@entry.Content</td>
					<td>@entry.Created</td>
					<td>Edit and Delete</td>
				</tr>
			}
		</tbody>
	</table>
</div>